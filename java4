package chap01;

public class chap01_ex {
    public static void main(String[] args) {
        String name = "감자바";
        int age = 25;
        String tel1="010", tel2="123", tel3="4567";
        System.out.println("이름: "+name);
        System.out.println("나이: "+age);
        System.out.println("전화: "+tel1+"-"+tel2+"-"+tel3);
    }
}
